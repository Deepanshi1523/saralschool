{
  "kind": "collectionType",
  "collectionName": "master_courses",
  "info": {
    "singularName": "master-course",
    "pluralName": "master-courses",
    "displayName": "Master Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Student_per_batch": {
      "type": "integer",
      "default": 15,
      "required": true
    },
    "cost": {
      "type": "biginteger",
      "required": true
    },
    "rating": {
      "type": "decimal",
      "default": 5,
      "max": 5,
      "min": 0,
      "required": true
    },
    "numberOfClasses": {
      "type": "integer",
      "required": true,
      "default": 25
    },
    "total_student": {
      "type": "biginteger",
      "default": "150",
      "required": true
    },
    "isHeroCourse": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "Scholarship": {
      "type": "integer",
      "required": false,
      "default": 0,
      "max": 100,
      "min": 0
    }
  }
}
