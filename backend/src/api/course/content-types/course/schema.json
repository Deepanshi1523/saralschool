      "required": true
    },
    "cost": {
      "type": "biginteger",
      "required": true
    },
    "numberOfClasses": {
      "type": "integer",
      "required": true
    },
    "total_student": {
      "type": "biginteger",
      "required": true
    },
    "isHeroCourse": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "image": {
      "type": "media",
